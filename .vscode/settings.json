{
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.ruff": "explicit",
            "source.organizeImports.ruff": "explicit"
        },
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true
    },
    "cSpell.words": [
        "addopts",
        "aiohttp",
        "asctime",
        "autodoc",
        "cafile",
        "codespell",
        "currentpage",
        "ddragon",
        "eeoc",
        "endpoit",
        "github",
        "Hamburg√£o",
        "iebrlh",
        "isort",
        "itemset",
        "itemsets",
        "lcumadesimple",
        "leaguewizard",
        "leaguewizardlock",
        "levelname",
        "minversion",
        "mobalytics",
        "mypy",
        "myst",
        "noconsole",
        "noqa",
        "onefile",
        "pipx",
        "PROCESSENTRY",
        "pycodestyle",
        "pydantic",
        "pydocstyle",
        "Pyflakes",
        "pyinstaller",
        "pyproject",
        "pystray",
        "pytest",
        "pyupgrade",
        "pywin",
        "riotgames",
        "selectolax",
        "simplerunes",
        "SNAPPROCESS",
        "Toolhelp",
        "typehints",
        "venv",
        "windll",
        "winerror"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "editor.formatOnSave": true,
    "editor.rulers": [
        88
    ],
    "evenBetterToml.formatter.columnWidth": 20,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.egg-info": true,
        "**/.coverage": true,
        "**/.mypy_cache": true,
        "**/.pdm-build": true,
        "**/.pytest_cache": true,
        "**/.ruff_cache": true,
        "**/.venv": true,
        "**/Pipfile*": true,
        "**/__pycache__": true,
        "**/_build": true,
        "**/coverage.xml": true,
        "**/htmlcov": true,
        ".git": true,
        ".venv": true,
        "coverage.xml": true,
        ".editorconfig": true,
        ".github": true,
        ".pre-commit-config.yaml": true,
        ".python-version": true,
        ".vscode": true,
        ".markdownlint.json": true,
        "mypy.ini": true,
        "LICENSE": true,
        "ruff.toml": true,
        "src/leaguewizard/py.typed": true,
        "docs": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "sortJSON.contextMenu": {
        "sortJSONAlphaNum": false,
        "sortJSONAlphaNumReverse": false,
        "sortJSONKeyLength": false,
        "sortJSONKeyLengthReverse": false,
        "sortJSONReverse": false,
        "sortJSONType": false,
        "sortJSONTypeReverse": false,
        "sortJSONValues": false,
        "sortJSONValuesReverse": false
    },
    "hide-files.files": [
        ".git",
        ".venv",
        "coverage.xml",
        ".editorconfig",
        ".github",
        ".pre-commit-config.yaml",
        ".python-version",
        ".vscode",
        ".markdownlint.json",
        "mypy.ini",
        "LICENSE",
        "ruff.toml",
        "src/leaguewizard/py.typed",
        "docs"
    ],
    "autoDocstring.docstringFormat": "google",
}
